{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "interface-over-type-literal": false,
        "object-literal-shorthand": false,
        "triple-equals": true,
        "no-empty": false,
        "class-name": false,
        "max-classes-per-file": false,
        "variable-name": false,
        "no-string-literal": false
    },
    "rulesDirectory": []
}
